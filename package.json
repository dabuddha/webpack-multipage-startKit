{
  "name": "webpack-static-html-scss",
  "version": "1.0.0",
  "description": "Webpack 4 starter kit for multipage static websites",
  "private": true,
  "scripts": {
    "new": "node createPage.js",
    "dev": "webpack-dev-server --host 0.0.0.0 --env.mode='development'",
    "dev:https": "webpack-dev-server --host 0.0.0.0 --https --env.mode='development'",
    "watch": "webpack --env.mode='development' --watch",
    "build": "webpack --env.mode='production'",
    "build:dev": "webpack --env.mode='simulation'",
    "postbuild": "html dist/*.html --indent-size 2"
  },
